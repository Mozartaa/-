/* pages/detail.wxss */
.detail{
  width:100%;
  height:100%;
}
.detail image{
  width:100%;
  opacity: 0.2;
}

.content {
  margin: 30rpx;
}

.content .icon image {
  width: 100rpx;
  height: 100rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.content .icon .text {
  font-size: 35rpx;
  font-weight: 700;
}
.content .item-list {
  border-bottom: 1px solid #f5f8f9;
  padding-bottom: 36rpx;
}

.list-text{
  font-size: 32rpx;
  color: #7e8793;
  background-color: #f5f8f9;
  line-height: 60rpx;
  margin-left: 35px;
  margin-right: 35px;
  padding: 5px 20px 5px 20px;
  display: inline-block;
  border-radius: 50rpx;
}

.content .back-btn {
  display: flex;
  justify-content: center;
  padding: 16rpx 32rpx;
  color: #5bd4c7;
  font-size: 36rpx;
  line-height: 56rpx;
  background-color: #f5f8f9;
  border-radius: 200rpx;
  margin: 0 16rpx 0 16rpx;
}