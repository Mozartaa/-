page {
  background-color: white;
}

.words {
  font-size: 30rpx;
  font-weight: bold;
  margin-left: 15px;
  margin-top: 15px;
  color: black;
}

.input {
  margin-top: 20rpx;
  font-size: 14px;
  color: black;
  margin-bottom: 10rpx;
}

.in {
  margin-left: 15px;
  margin-top: 0px;
  height: 8px;
}

.picker{
  font-size: 28rpx;
}

.line {
  margin-top: 15px;
  width: 95%;
  height: 3px;
  margin-left: 2.5%;
  background: -webkit-linear-gradient(left, rgb(0, 6, 129), rgb(91, 212, 199));
  background: linear-gradient(left, rgb(0, 6, 129), rgb(91, 212, 199));
}

.img-word {
  font-size: 14px;
  margin-left: 15px;
  margin-top: 15px;
  color: #888888;
  margin-bottom: 15px;
}

.sub {
  display: flex;
  justify-content: center;
  padding: 16rpx 32rpx;
  font-size: 36rpx;
  line-height: 48rpx;
  background-color: #f5f8f9;
  border-radius: 200rpx;
  margin: 25px 20px 35px 20px;
  border: 1px solid #fff;
  background: -webkit-linear-gradient(left, rgb(0, 6, 129), rgb(91, 212, 199));
  background: linear-gradient(left, rgb(0, 6, 129), rgb(91, 212, 199));
}

.subwords {
  color: white;
}

.image-group-list {
  padding-top: 20rpx;
  display: flex;
  justify-content: start;
  flex-wrap: wrap;
  margin-left: 10%;
}

.image-group {
  margin-right: 5rpx;
  margin-top: 5rpx;
  position: relative;
  /* box-sizing: border-box; */
}

.image-group .close-btn {
  position: absolute;
  right: -30rpx;
  top: -30rpx;
  width: 60rpx;
  height: 60rpx;
  z-index: 1;
  background-size: 40rpx;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}

.add-image-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 200rpx;
  color: #a9a9a9;
  background: #f2f2f2;
  margin-top: 5rpx;
}

.add-image-btn view {
  margin-top: -20rpx;
}

.image-group image {
  width: 100%;
  height: 100%;
}

.subwords {
  color: white;
  background-color: transparent;
  width: 100%;
}

button::after {
  border: none;
}
