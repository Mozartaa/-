.searchbar{
  background-color: #fff;
  padding: 20rpx;
  margin-top:0.5cm;
}

.search-navigator{
  width: 100%;
  height: 70rpx;
  background-color:#D3D3D3;
  box-shadow: -4px 10px 10px black;
  border-radius: 10rpx;
  background-image: url("data:image/png;base64,");
  background-position: 2% center;
  background-repeat: no-repeat;
  background-size: 7%;
}

.search-input-group{
  width: 100%;
  height: 70rpx;
  background-color:#D3D3D3;
  box-shadow: -4px 10px 10px black;
  border-radius: 10rpx;
  padding:5rpx 20rpx;
  box-sizing:border-box;
}

.search-input{
  min-height: 40rpx;
  height: 40rpx;
  font-size: 1px;
}

.teaformation{
  border-radius: 10rpx;
  width: 95%;
  height: 210rpx;
  box-shadow: -2px 5px 4px rgba(0, 0, 0, 0.925);
  background: rgba(66, 63, 63, 0.842);
  margin-left:2.5%;
  margin-top:20rpx;
  display: flex;
  flex-direction: column;
  /* align-content: center; */
}

.teaformation .name{
  height: 70rpx;
  margin-left: 10rpx;
  color: white;
  padding-left: 2px;
  padding-top: 10rpx;
  text-overflow: ellipsis;
  overflow: hidden;
}
.teaformation .dep{
  height: 50rpx;
  margin-left: 10rpx;
  color: white;
  padding-left: 2px;
  text-overflow: ellipsis;
  overflow: hidden;
}
.teaformation .homepage{
  height: 70rpx;
  margin-left: 10rpx;
  margin-top: 20rpx;
  color: white;
  padding-left: 2px;
  text-overflow: ellipsis;
  overflow: hidden;
}

.scroll-view-y{
  width:100%;
  height: 520px;
  margin-top:50px;
  background: white;
}

.mask{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0,0,0,0.3);
  z-index: 1;
}